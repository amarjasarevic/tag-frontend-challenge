{
  "devDependencies": {
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2"
  },
  "name": "tag-frontend-challenge",
  "description": "These tests will be used to evaluate your skillset and the type of tasks/projects you'd be great for. We encourage you to do both tests. They are different in nature and evaluate different skills. Start with one that better demonstrates your craft.",
  "version": "1.0.0",
  "main": ".eslintrc.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amarjasarevic/tag-frontend-challenge.git"
  },
  "author": "amar.jasarevic@toptal.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amarjasarevic/tag-frontend-challenge/issues"
  },
  "homepage": "https://github.com/amarjasarevic/tag-frontend-challenge#readme",
  "lint-staged": {
    "*.js, *.json, *.scss, *.html": [
      "npm run lint:fix",
      "npm run prettier:fix"
    ]
  }
}
